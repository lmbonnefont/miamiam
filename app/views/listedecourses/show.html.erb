<div class="container">
  <div class="row">
    <% @h.each do |key, value| %>
      <% if !@notdisplayed.include? key %>
        <div class="col-xs-4">
          <div class="ingredient">
            <%= value.ceil %> <%= key %>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>

